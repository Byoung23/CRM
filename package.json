{
  "name": "crm",
  "version": "1.0.0",
  "engines": {
    "node": "10.3.0"
  },
  "description": "CRM",
  "main": "server.js",
  "proxy": "http://0.0.0.0:3001",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Dane Shrewsbury",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "material-ui": "^0.20.2",
    "mongoose": "^5.6.11",
    "mongoose-auto-increment": "^5.0.1",
    "mui-datatables": "^2.8.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "react-trello": "2.2.3",
    "validator": "^11.1.0"
  }
}
